# include <stdio.h>

int print(int a)
{
	int i, j;
	for(i = 0; i < a; i++)
	{
		for(j = 0; j <= i; j++)
		{
			printf("%d", j + 1);
		}
		printf("\n");
	}
}


int main(void)
{
	int a;
	scanf("%d", &a);
	print(a);
	return 0;
}
--------------------------------------------------------------

# include <stdio.h>

void print(int count)
{
	if(count == 0)
	{
		return;
	}
	else
	{
		printf("문자열을 출력합니다.\n");
		print(count - 1);
	}
}



int main(void)
{
	int number;
	printf("문자열을 몇 개 출력할까요?");
	scanf("%d", &number);
	print(number);
	return 0;
}
--------------------------------------------------------------

# include <stdio.h>

int nCr(int n, int r)
{
	if(r == 0 || r == n)
	{
		return 1;
	}
	else
	{
		return nCr(n - 1, r - 1) + nCr(n - 1, r);
	}
}


int main(void)
{
	int n, r;
	scanf("%d %d", &n, &r);
	printf("%d", nCr(n, r));
	return 0;
}
