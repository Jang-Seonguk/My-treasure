# include <stdio.h>

struct point {
	int x;
	int y;
};

void comparePoint (struct point p1, struct point p2) {
	if((p1.x == p2.x) && (p1.y == p2.y)) 
	{
		printf("p1과 p2가 같습니다.");
	}	
}

int main(void)
{
	struct point p1;
	struct point p2;
	
	p1.x = 30;
	p1.y = 10;
	
	p2.x = 30;
	p2.y = 10;
		
	comparePoint(p1, p2);
	return 0;
}
----------------------------------------------------------------------------

# include <stdio.h>
# define SIZE 5

struct student {
	int number;
	char name[20];
	double grade;
	
};

int main(void)
{
	struct student list[SIZE];
	int i;
	
	for(i = 0; i < SIZE; i++)
	{
		printf("학번을 입력하세요 : ");
		scanf("%d", &list[i].number);
		printf("이름을 입력하세요 : ");
		scanf("%s", &list[i].name);
		printf("학점을 입력하세요 : ");
		scanf("%lf", &list[i].grade);
	}
	
	for(i = 0; i < SIZE; i++)
	{
		printf("학번 : %d, 이름 : %s, 학점 %.1f\n", list[i].number, list[i].name, list[i].grade);
	}

	return 0;
}
